{
  "name": "Relp Cell Pagamentos",
  "short_name": "Relp Cell",
  "description": "Gerencie suas faturas, faça compras e acompanhe seu limite na Relp Cell com facilidade e segurança.",
  "id": "relp-cell-pagamentos-v1",
  "start_url": "/",
  "scope": "/",
  "display": "standalone",
  "display_override": ["tabbed", "window-controls-overlay", "minimal-ui"],
  "background_color": "#f8fafc",
  "theme_color": "#4f46e5",
  "orientation": "portrait-primary",
  "lang": "pt-BR",
  "dir": "ltr",
  "categories": ["finance", "shopping", "productivity", "utilities"],
  "icons": [
    {
      "src": "/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "screenshots": [
    {
      "src": "https://placehold.co/750x1334/4f46e5/ffffff.png?text=Inicio+Relp+Cell",
      "sizes": "750x1334",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Tela Inicial"
    },
    {
      "src": "https://placehold.co/750x1334/ffffff/4f46e5.png?text=Faturas",
      "sizes": "750x1334",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Minhas Faturas"
    },
    {
      "src": "https://placehold.co/1280x800/4f46e5/ffffff.png?text=Relp+Cell+Desktop",
      "sizes": "1280x800",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Versão Desktop"
    }
  ],
  "shortcuts": [
    {
      "name": "Minhas Faturas",
      "short_name": "Faturas",
      "description": "Visualize e pague suas faturas",
      "url": "/?tab=faturas",
      "icons": [{"src": "/icons/icon-192x192.png", "sizes": "192x192", "type": "image/png"}]
    },
    {
      "name": "Loja Online",
      "short_name": "Loja",
      "description": "Confira as ofertas",
      "url": "/?tab=loja",
      "icons": [{"src": "/icons/icon-192x192.png", "sizes": "192x192", "type": "image/png"}]
    }
  ],
  "share_target": {
    "action": "/?share=true",
    "method": "GET",
    "enctype": "application/x-www-form-urlencoded",
    "params": {"title": "title", "text": "text", "url": "url"}
  },
  "launch_handler": {"client_mode": "focus-existing"},
  "protocol_handlers": [{"protocol": "web+relp", "url": "/?type=%s"}],
  "scope_extensions": [
    {"origin": "*.relpcell.com.br"},
    {"origin": "*.vercel.app"}
  ],
  "file_handlers": [
    {
      "action": "/?open-file",
      "accept": {
        "application/pdf": [".pdf"],
        "text/csv": [".csv"],
        "application/vnd.ms-excel": [".xls", ".xlsx"]
      }
    }
  ],
  "widgets": [
    {
      "name": "Resumo Financeiro",
      "short_name": "Resumo",
      "description": "Veja seu saldo",
      "tag": "financial-summary",
      "template": "financial-summary",
      "data": "/?widget=summary",
      "type": "application/json",
      "screenshots": [
        {
          "src": "/icons/icon-192x192.png",
          "sizes": "192x192",
          "label": "Widget"
        }
      ]
    }
  ],
  "note_taking": {"new_note_url": "/?note=new"},
  "prefer_related_applications": false
}
